package ej732granja;

public class GranjaPeano {

	public static void main(String[] args) {

	}

	public static int calcularPuertas(int[] posIni, int[] posFi) {

		int nPuertas = 0;

		int[] celdaIni = { posIni[0] / 3, posIni[1] / 3 };

		System.out.println(celdaIni);

		int[] celdaFi = { posFi[0] / 3, posFi[1] / 3 };

		System.out.println(celdaFi);

		return nPuertas;
	}

}
